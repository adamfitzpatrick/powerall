{
  "name": "powerall-web",
  "version": "1.0.0",
  "description": "Simple static website to display captured power source measurement data",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "tdd": "ts-node -r tsconfig-paths/register ./node_modules/.bin/tape-watch ./src/**/*.spec.ts* | faucet",
    "test": "nyc tape ./src/**/*.spec.ts*"
  },
  "author": "Adam Fitzpatrick",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^9.5.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-select": "^3.0.11",
    "@types/sinon": "^7.5.2",
    "@types/tape": "^4.2.34",
    "@types/uuid": "^7.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "faucet": "0.0.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "moment": "^2.24.0",
    "nyc": "^15.0.0",
    "react": "^16.13.0",
    "react-chart": "0.0.1",
    "react-dom": "^16.13.0",
    "recharts": "2.0.0-beta.5",
    "resource-hints-webpack-plugin": "0.0.2",
    "sinon": "^9.0.1",
    "standard": "^14.3.1",
    "tap-diff": "^0.1.1",
    "tape": "^4.13.2",
    "tape-watch": "^2.3.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "nyc": {
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.tsx",
      "src/**/*.spec.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "all": true,
    "check-coverage": true
  }
}
